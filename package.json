{
  "name": "@gravitate-health/lens-tool-lib",
  "version": "1.0.0",
  "description": "Helper library for developing Gravitate Health lens components",
  "main": "src/index.js",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:verbose": "jest --verbose"
  },
  "keywords": [
    "gravitate-health",
    "lens",
    "fhir",
    "healthcare",
    "epi",
    "ips"
  ],
  "author": "Gravitate Health",
  "license": "Apache-2.0",
  "dependencies": {
    "jsdom": "^20.0.0"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "jest": "^30.2.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gravitate-health/lens-tool-lib.git"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "jest": {
    "testEnvironment": "node",
    "collectCoverageFrom": [
      "src/**/*.js",
      "!src/index.js"
    ],
    "coverageDirectory": "coverage",
    "testMatch": [
      "**/test/**/*.test.js"
    ],
    "verbose": true
  }
}
